{
    "name": "pratikid/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pratik Khanolkar",
            "email": "pratikid@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.9",
        "illuminate/support": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/pagination": "5.7.*",
        "illuminate/events": "5.7.*",
        "illuminate/console": "5.7.*",
        "heydavid713/neo4jphp": "0.1.*",
        "nesbot/carbon": "~1.0",
		"doctrine/dbal": "^2.6",
		"dragonmantank/cron-expression": "^2.0",
		"fzaninotto/faker": "^1.4",
		"guzzlehttp/guzzle": "^6.0",
		"illuminate/filesystem": "5.7.*",
		"moontoast/math": "^1.1",
		"ramsey/uuid": "^3.7",
		"symfony/config": "*",
		"symfony/event-dispatcher": "*",
		"symfony/lock": "*",
		"symfony/process": "*",
		"symfony/var-dumper": "^4.1",
		"symfony/yaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "1.0"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    }
}
